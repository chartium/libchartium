{
  "name": "libchartium",
  "version": "0.0.32",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "svelte": "./dist/lib/index.js"
    },
    "./worker": {
      "types": "./dist/lib/data-worker/worker.d.ts",
      "import": "./dist/lib/data-worker/worker.js"
    },
    "./wasm": {
      "types": "./dist/wasm/libchartium.wasm.d.ts",
      "import": "./dist/wasm/libchartium.wasm"
    }
  },
  "scripts": {
    "dev": "pnpm build-wasm && vite dev",
    "build": "pnpm build-wasm && svelte-package -i src/lib -o dist/lib",
    "build-wasm": "sh ./build-wasm.sh",
    "clean": "sh ./clean.sh",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.5",
    "@sveltejs/vite-plugin-svelte": "^2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.11.0",
    "prettier": "3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte-check": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@mod.js/signals": "^0.0.18",
    "@risai/bim": "^1.4.2",
    "@types/dom-to-image": "^2.6.7",
    "@types/lodash-es": "^4.17.12",
    "@types/seedrandom": "^3.0.8",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.10",
    "dom-to-image-more": "^3.2.0",
    "lodash-es": "^4.17.21",
    "random": "^4.1.0",
    "sass": "^1.69.7",
    "seedrandom": "^3.0.5",
    "svelte": "^4.2.8",
    "svelte-fa": "^4.0.1",
    "svelte-portal": "^2.2.0",
    "unitlib": "^0.5.2"
  }
}
