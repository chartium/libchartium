    "prettier": "3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte-check": "^3.6.9",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@mod.js/signals": "^0.0.30",
    "@risai/bim": "^1.4.2",
    "@tanstack/svelte-virtual": "^3.4.0",
    "@types/lodash-es": "^4.17.12",
    "@types/seedrandom": "^3.0.8",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.10",
    "html-to-image": "^1.11.11",
    "lodash-es": "^4.17.21",
    "random": "^4.1.0",
    "sass": "^1.75.0",
    "seedrandom": "^3.0.5",
    "svelte": "^4.2.15",
    "svelte-fa": "^4.0.2",
    "svelte-portal": "^2.2.1",
    "svelte-typed-context": "^1.0.1",
    "ts-pattern": "^5.1.1",
    "yeet-ts": "^1.0.3"
  },
  "peerDependencies": {
    "fraction.js": "^4.3.7",
    "unitlib": "^0.8.1"
  }
