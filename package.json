{
  "name": "libchartium",
  "version": "0.0.19",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "svelte": "./dist/lib/index.js"
    },
    "./worker": {
      "types": "./dist/lib/data-worker/worker.js",
      "import": "./dist/lib/data-worker/worker.d.ts"
    },
    "./wasm": {
      "types": "./dist/wasm/libchartium.wasm.d.ts",
      "import": "./dist/wasm/libchartium.wasm"
    }
  },
  "scripts": {
    "dev": "pnpm build-wasm && vite",
    "build": "pnpm build-wasm && svelte-package -i src/lib -o dist/lib",
    "build-wasm": "sh ./build-wasm.sh",
    "clean": "sh ./clean.sh",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.8.10",
    "svelte-check": "^3.5.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@mod.js/signals": "^0.0.13",
    "@types/dom-to-image": "^2.6.6",
    "@types/lodash-es": "^4.17.10",
    "@types/seedrandom": "^3.0.7",
    "bim": "^1.3.3",
    "comlink": "^4.4.1",
    "dayjs": "^1.11.10",
    "dom-to-image-more": "^3.2.0",
    "lodash-es": "^4.17.21",
    "random": "^4.1.0",
    "sass": "^1.69.5",
    "seedrandom": "^3.0.5",
    "svelte": "^4.2.2",
    "svelte-fa": "^3.0.4",
    "svelte-portal": "^2.2.0",
    "unitlib": "^0.4.7"
  }
}
